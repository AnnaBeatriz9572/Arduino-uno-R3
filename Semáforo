#include <Arduino.h>

int red = 10;
int yellow = 11;
int green = 12;
void setup() {
  pinMode(red, OUTPUT);
  pinMode(yellow, OUTPUT);
  pinMode(green, OUTPUT);
}

void loop() {
  digitalWrite(red, HIGH);
  delay(5000);
  digitalWrite(red, LOW);
  digitalWrite(yellow, HIGH);
  delay(2000);
  digitalWrite(yellow, LOW);
  digitalWrite(green, HIGH);
  delay(5000);
  digitalWrite(green, LOW);
}
// nesse eu usei todos os leds da mesma cor de 5mm
// resistores de 100 ohms
// protoboard
//jumpers

// se for de cores diferentes aconselho resistores de 150 ohms
